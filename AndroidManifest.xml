	package="net.armooo.locationlog"
	android:versionName="1.0.3"
	android:versionCode="4">

    <uses-sdk
        android:minSdkVersion="2"/>

	<application
		android:icon="@drawable/icon"
		android:label="@string/app_name">

		<activity
			android:name=".LocationListActivity"
			android:label="@string/app_name">
